<p class="control">
	<label class="checkbox">
	<input name="<%= commandData.name %>-isEnabled" type="checkbox" onclick="$('.<%= commandData.name %>-options').toggleClass('is-hidden');" <%= commandData.isEnabled ? "checked" : "" %>>
		<strong><%= commandData.name %></strong> <%= commandData.description %>
	</label>
	<span class="<%= commandData.name %>-options <%= commandData.isEnabled ? '' : 'is-hidden' %>">
		<span class="select is-small">
			<select name="<%= commandData.name %>-isAdminOnly">
				<option value="false"<%= commandData.isAdminOnly ? "" : " selected" %>>@everyone</option>
				<option value="true"<%= commandData.isAdminOnly ? " selected" : "" %>>Admin-only</option>
			</select>
		</span>
		<br>
		<% for(var i=0; i<channelData.length; i++) { %>
			&nbsp;&nbsp;&nbsp;
			<label class="checkbox">
			<input name="<%= commandData.name %>-disabled_channel_ids-<%= channelData[i].id %>" type="checkbox" <%= commandData.disabled_channel_ids.indexOf(channelData[i].id)==-1 ? "checked" : "" %>>
				#<%= channelData[i].name %>
			</label>
			<br>
		<% } %>
	</span>
</p>